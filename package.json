{
  "name": "ninjas-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-dev": "DEBUG=ninjas-node:* nodemon ./bin/www",
    "start": "node ./bin/www",
    "watch": "mocha -w 'tests/**/*.test.js'",
    "test": "mocha --timeout 10000 'tests/**/*.test.js'",
    "test-coverage": "nyc --reporter=html mocha --timeout 10000 'tests/**/*.test.js'",
    "migrate": "sequelize db:migrate",
    "migrate-undo": "sequelize db:migrate:undo"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.2",
    "helmet": "^3.16.0",
    "http-errors": "^1.7.2",
    "js-yaml": "^3.13.0",
    "morgan": "~1.9.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.3",
    "faker": "^4.1.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4"
  }
}
